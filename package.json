{
  "name": "esmbot",
  "version": "1.10.3",
  "description": "A Discord bot with miscellaneous features",
  "exports": "./build/app.js",
  "bin": "./build/app.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc && pnpm build:natives",
    "build:debug": "tsc && pnpm build:natives:debug",
    "build:natives": "cmake-js compile --CDWITH_MAGICK=ON",
    "build:natives:debug": "cmake-js compile -BDebug --CDWITH_MAGICK=ON",
    "build:natives:no-magick": "cmake-js compile --CDWITH_MAGICK=OFF",
    "build:natives:debug-no-magick": "cmake-js compile -BDebug --CDWITH_MAGICK=OFF",
    "docker:build": "DOCKER_BUILDKIT=1 docker build -t esmbot .",
    "docker:run-bot": "docker run --rm --network=host esmbot",
    "docker:run-api": "docker run --rm --network=host esmbot-api",
    "docker:run-lava": "docker run --rm --network host -v \"$(pwd)\"/application.yml:/opt/Lavalink/application.yml -v \"$(pwd)\"/assets:/opt/Lavalink/assets fredboat/lavalink:dev",
    "start": "node build/app.js",
    "start:debug": "DEBUG=true node build/app.js",
    "start-api": "node api/index.js",
    "start-api:debug": "DEBUG=true node build/app.js"
  },
  "author": "Essem <essem@essem.space>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esmBot/esmBot.git"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "emoji-regex": "^10.3.0",
    "file-type": "^18.7.0",
    "format-duration": "^3.0.2",
    "jsqr": "^1.4.0",
    "node-addon-api": "^7.0.0",
    "oceanic.js": "1.8.1",
    "qrcode": "^1.5.3",
    "sharp": "^0.32.6",
    "shoukaku": "github:shipgirlproject/Shoukaku",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/eslint-plugin": "^7.23.5",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@biomejs/biome": "1.4.0",
    "@biomejs/cli-linux-x64": "1.4.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^18.19.3",
    "cmake-js": "^7.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.2.0",
    "pm2": "^5.3.0",
    "postgres": "^3.4.3",
    "ws": "^8.14.2",
    "zlib-sync": "^0.1.9"
  },
  "binary": {
    "napi_versions": [
      7
    ]
  },
  "imports": {
    "#cmd-classes/*": "./build/classes/*",
    "#utils/*": "./build/utils/*",
    "#database": "./build/database.js",
    "#pagination": "./build/pagination/pagination.js",
    "#config/*": "./config/*"
  },
  "packageManager": "pnpm@8.12.1"
}
